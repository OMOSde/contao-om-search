{
  "name": "omosde/contao-om-search",
  "type": "contao-bundle",
  "description": "Contao 5.2+ module with additional search features",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Ren√® Fehrmann",
      "homepage": "http://www.omos.de"
    }
  ],
  "require": {
    "php": "^8.2",
    "contao/core-bundle": "^5.2"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "doctrine/doctrine-cache-bundle": "^1.3"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "OMOSde\\ContaoOmSearchBundle\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "OMOSde\\ContaoOmSearchBundle\\ContaoManager\\Plugin"
  }
}
